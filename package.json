{
  "name": "@huluvu424242/honey-speech",
  "version": "0.0.8",
  "description": "Text to Speech component wich is reading texts from DOM elements.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/honey-speech/honey-speech.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "engines": {
    "node": "10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FunThomas424242/honey-speech-component.git"
  },
  "keywords": [
    "honey",
    "speech",
    "speech-output",
    "text2speech",
    "webcomponent",
    "custom-element"
  ],
  "author": "FunThomas424242 alias Huluvu424242 (Thomas Schubert)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FunThomas424242/honey-speech-component/issues"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepare": "npm run build && npm run storybook-build",
    "deliver": "npm publish --access=public",
    "storybook-run": "start-storybook -p 9009 -s www/build/assets",
    "storybook-run-old": "start-storybook -p 9009 -s dist/collection/components/honey-speech",
    "storybook-build": "build-storybook -c .storybook -o docs -s dist/collection/components/honey-speech/assets"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@stencil/core": "1.8.9",
    "@stencil/webpack": "0.0.6",
    "@storybook/html": "5.3.13",
    "@storybook/preset-typescript": "1.2.0",
    "@storybook/addon-info": "5.3.13",
    "@types/jest": "24.0.25",
    "@types/puppeteer": "1.20.3",
    "@types/webpack": "4.41.6",
    "babel-loader": "8.0.6",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "1.20.0",
    "react-docgen-typescript-loader": "3.6.0",
    "ts-loader": "6.2.1"
  }
}
